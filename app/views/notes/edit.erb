<div class="d-flex justify-content-center text-center">

<h1> Edit note </h1>

</div>

<div class="d-flex justify-content-center text-center">

<%= form_for @note do |f| %>
    <%=f.label :title %><br>
    <%=f.text_field :title %><br>

    <%=f.label :content %><br>
    <%=f.text_field :content %><br>

    <%=f.submit "Submit"%>
<% end %>

</div>